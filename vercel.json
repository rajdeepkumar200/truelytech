{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/habitency\\.apk",
      "dest": "/habitency.apk",
      "headers": {
        "Content-Type": "application/vnd.android.package-archive",
        "Content-Disposition": "attachment; filename=habitency.apk",
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/app-update\\.json",
      "dest": "/app-update.json",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/sw\\.js",
      "dest": "/sw.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/manifest\\.webmanifest",
      "dest": "/manifest.webmanifest",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    { "src": "/install", "dest": "/install/index.html" },
    { "src": "/install/", "dest": "/install/index.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
