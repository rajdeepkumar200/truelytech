{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": false,
  "framework": "vite",
  "headers": [
    {
      "source": "/habitency.apk",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=habitency.apk"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/habitency.apk",
      "destination": "/habitency.apk"
    },
    {
      "source": "/app-update.json",
      "destination": "/app-update.json"
    },
    {
      "source": "/((?!habitency.apk|app-update.json|assets/|favicon|sw.js|workbox).*)",
      "destination": "/index.html"
    }
  ]
}
